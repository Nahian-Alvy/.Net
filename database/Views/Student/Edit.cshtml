
@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>
<form method="post">
    <input a type="hidden" name="Id" value="@Model.id" />
    <input type="text" name="Name" value="@Model.name"class=" form-control" placeholder="Name" />
    <input type="date" name="Dob" value="@Model.dob"class=" form-control" placeholder="Dob" />
    <input type="text" name="Cgpa" value="@Model.cgpa"class=" form-control" placeholder="Cgpa" />
    <input type="submit" value="Edit" class="btn btn-primary" />
</form>
